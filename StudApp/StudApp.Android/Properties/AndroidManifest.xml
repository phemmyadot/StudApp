<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:versionCode="1" android:versionName="1.0" package="com.eminent.studentapp">
    <uses-sdk android:minSdkVersion="21" android:targetSdkVersion="31" />
    <application android:label="StudApp.Android" android:theme="@style/MainTheme" android:usesCleartextTraffic="true">
		<activity android:configChanges="orientation|smallestScreenSize|screenLayout|screenSize|uiMode" android:icon="@mipmap/icon" android:label="StudentApp" android:theme="@style/MainTheme" android:name="crc64301dd61589194418.MainActivity" android:exported="true">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
			<!--SEND INTENT FILTER-->
			<intent-filter>
				<action android:name="android.intent.action.SEND" />
				<category android:name="android.intent.category.DEFAULT" />
				<data android:mimeType="text/plain" />
			</intent-filter>
			<!--VIEW INTENT FILTER-->
			<intent-filter>
				<action android:name="android.intent.action.VIEW" />
